{
  "extends": ["airbnb", "airbnb/hooks"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "_baseUrl", "_checkResponse"] }],
    "react/prop-types": "off",
    "react/jsx-no-bind": "off",
    "prefer-promise-reject-errors": "off",
    "no-undef": "off",
    "class-methods-use-this": "off"
  }
}
